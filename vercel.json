{
  "version": 2,
  "name": "checkout-syncpay",
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "index.html",
        "css/**",
        "js/**",
        "fonts/**",
        "images/**",
        "media/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
